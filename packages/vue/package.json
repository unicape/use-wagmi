  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "typescript": ">=5.0.4",
    "viem": ">=0.3.35",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@tanstack/query-persist-client-core": "^4.29.11",
    "@tanstack/query-sync-storage-persister": "^4.29.11",
    "@tanstack/vue-query": "^4.29.11",
    "@wagmi/core": "^1.3.2",
    "@wagmi/chains": "^1.3.0",
    "@wagmi/connectors": "^2.6.1",
    "eventemitter3": "^4.0.7",
    "abitype": "0.8.7",
